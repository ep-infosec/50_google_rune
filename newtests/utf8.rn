// Rune allows source code to be written in UTF-8.  Identifiers can also be in UTF-8, if escaped.
// Even keywords can be identifiers.

hässlich = "Μεδουσα"
schön = "Ἀφροδίτη"
\. = "."
println schön , " is prettier than ", hässlich , \.
