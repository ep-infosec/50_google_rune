class Foo(self) {
  self.x = 1
}

generator addToClass(A: Class, label:string) {
  appendcode A {
    self.y = 2
    self.$label$A_label = "baz"

    func dump(self) {
      println self.$label$A_label, " = ", self.x, ", ", self.y
    }
  }
}

generate addToClass(Foo, "bar")

f = Foo()
f.dump()
