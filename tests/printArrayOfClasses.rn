class Foo(self) {
  self.bars = arrayof(Bar(self, u64))
}

class Bar:u16 (self, foo: Foo, val: u64) {
  self.val = val
  foo.bars.append(self)
}

foo = Foo()
Bar(foo, 1)
Bar(foo, 2)
Bar(foo, 3)
println foo
