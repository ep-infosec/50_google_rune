class Point1(self, x, y) {
  self.x = x
  self.y = y

  func toString(self) {
    return "{x = %u, y = %u}" % (self.x, self.y)
  }
}

class Point2(self, x, y) {
  self.x = x
  self.y = y
}

// This one calls the user-defined toString.
p1 = Point1(10, 15)
println p1

// This one calls the auto-generated toString.
p2 = Point2(10, 15)
println p2
println "%s" % p2.toString()
